<?xml version="1.0"?>

<robot name="suturo_dishwasher" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:property name="pi" value="3.1415926535897931" />

  <xacro:macro name="suturo_dishwasher" params="name parent *origin">
     <link name="${name}:dishwasher_main">

      <xacro:sphere_inertia radius="0" mass="0"/>
      <visual>
        <origin xyz="0 0 0.148" rpy="0 0 ${pi}" />
        <geometry>
          <mesh filename="package://iai_kitchen/meshes/misc/DishWasher.dae" scale="1.1 1.2 1.1"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0.148" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iai_kitchen/meshes/misc/DishWasher.obj"  scale="1.1 1.2 1.1"/>
        </geometry>
      </collision>
    </link>

    <link name="${name}:dishwasher_door">

      <xacro:sphere_inertia radius="0" mass="0"/>
      <visual>
        <origin xyz="0 -0.108 0" rpy="${0.5*pi} 0 0" />
        <geometry>
          <mesh filename="package://iai_kitchen/meshes/misc/DishWasherDoor.dae"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 -0.108 0" rpy="${0.5*pi} 0 0" />
        <geometry>
          <mesh filename="package://iai_kitchen/meshes/misc/DishWasherDoor.obj"/>
        </geometry>
      </collision>
    </link>

    <link name="${name}:dishwasher_door_handle">
      <xacro:sphere_inertia radius="0" mass="0"/>
      <visual>
        <origin xyz="0 0 0" rpy="0 ${pi} 0" />
        <geometry>
          <mesh filename="package://iai_kitchen/meshes/handles/Handle60.dae"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 ${pi} 0" />
        <geometry>
          <mesh filename="package://iai_kitchen/meshes/handles/Handle60.obj"/>
        </geometry>
      </collision>
    </link>

	<link name="${name}:dishwasher_tab"/>

	<!--
    <xacro:if value="simulated"/>
    -->
	<joint name="${name}:dishwasher_tab_joint" type="fixed">
	  <origin xyz="0.18 -0.33 0.1775" rpy="0 0 0" />
	  <parent link="${name}:dishwasher_door"/>
      <child link="${name}:dishwasher_tab"/>
	</joint>
    <joint name="${name}:dishwasher_main_joint" type="fixed">
      <xacro:insert_block name="origin" />
      <parent link="${parent}"/>
      <child link="${name}:dishwasher_main"/>
    </joint>

    <!--
    <xacro:if value="${simulated}">
    <joint name="${name}:dishwasher_door_joint" type="revolute">
      <origin xyz="0.27 0. -0.325" rpy="${-0.5*pi} 0 0" />
      <parent link="${name}:dishwasher_main"/>
      <child link="${name}:dishwasher_door"/>
      <axis xyz="0 0 1"/>
       FIXME make closed position stable instead of forbidding opening...
      <limit lower="0" upper="0" effort="300" velocity="10"/>
    </joint>

    </xacro:if>
    -->
    <!--
    <xacro:unless value="${simulated}">
    -->
    <joint name="${name}:dishwasher_door_joint" type="revolute">
      <origin xyz="-0.27 0. -0.285" rpy="${-0.5*pi} 0 0" />   <!-- <origin xyz="-0.085 0. -0.13" rpy="${-0.5*pi} -1.57 0" /> -->
      <parent link="${name}:dishwasher_main"/>
      <child link="${name}:dishwasher_door"/>
      <axis xyz="0 0 1"/>
      <!--
      <limit lower="0" upper="${0.5*pi}" effort="300" velocity="10"/>
      -->
      <limit lower="${-0.5*pi}" upper="0" effort="300" velocity="10"/>
    </joint>
    <!--
    </xacro:unless>
  -->
    <joint name="${name}:dishwasher_door_handle_joint" type="fixed">
      <origin xyz="-0.08 -0.728 0.0" rpy="${pi/2} 0 0" />
      <parent link="${name}:dishwasher_door"/>
      <child link="${name}:dishwasher_door_handle"/>
    </joint>

    <xacro:dishwasher_tray name="${name}:dishwasher_tray" parent="${name}:dishwasher_main">
      <origin xyz="-0.08 0 0.26"/>
    </xacro:dishwasher_tray>
    
    <xacro:dishwasher_tray name="${name}:dishwasher_tray_2" parent="${name}:dishwasher_main">
      <origin xyz="-0.08 0 -0.07"/>
    </xacro:dishwasher_tray>

  </xacro:macro>

  <xacro:macro name="dishwasher_tray" params="name parent *origin">

    <joint name="${name}_main" type="prismatic">
      <xacro:insert_block name="origin"/>
      <parent link="${parent}"/>
      <child link="${name}_bottom"/>
      <axis xyz="1 0 0"/>
      <limit lower="-0.33" upper="0" effort="300" velocity="10"/>
    </joint>


    <link name="${name}_bottom">
      <visual>
        <geometry>
          <box size="0.32 0.5 0.05"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <box size="0.32 0.5 0.05"/>
        </geometry>
      </collision>
    </link>

    <link name="${name}_left_side">
      <visual>
        <geometry>
          <box size="0.32 0.005 0.17"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <box size="0.32 0.005 0.17"/>
        </geometry>
      </collision>
    </link>

    <link name="${name}_right_side">
      <visual>
        <geometry>
          <box size="0.32 0.005 0.17"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <box size="0.32 0.005 0.17"/>
        </geometry>
      </collision>
    </link>

    <link name="${name}_back_side">
      <visual>
        <geometry>
          <box size="0.005 0.5 0.17"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <box size="0.005 0.5 0.17"/>
        </geometry>
      </collision>
    </link>

    <link name="${name}_front_side">
      <visual>
        <geometry>
          <box size="0.005 0.5 0.17"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <box size="0.005 0.5 0.17"/>
        </geometry>
      </collision>
    </link>

    <joint name="${name}_bottom_left" type="fixed">
      <origin xyz="-0.0 -0.25 0.075"/>
      <child link="${name}_left_side" />
      <parent link="${name}_bottom"/>
    </joint>

    <joint name="${name}_bottom_right" type="fixed">
      <origin xyz="-0.0 0.25 0.075"/>
      <child link="${name}_right_side" />
      <parent link="${name}_bottom"/>
    </joint>

    <joint name="${name}_bottom_back" type="fixed">
      <origin xyz="0.16 0 0.075"/>
      <child link="${name}_back_side" />
      <parent link="${name}_bottom"/>
    </joint>

    <joint name="${name}_bottom_front" type="fixed">
      <origin xyz="-0.16 0 0.075" rpy="0 0 0"/>
      <child link="${name}_front_side" />
      <parent link="${name}_bottom"/>
    </joint>

    <xacro:tray_handle name="${name}_handle" parent="${name}_front_side">
      <origin xyz="-0.03 -0.1 -0.02"/>
    </xacro:tray_handle>

  </xacro:macro>
  
